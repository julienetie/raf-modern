{
  "name": "request-frame-modern",
  "version": "2.0.0",
  "description": "Optimal requestAnimationFrame & cancelAnimationFrame polyfill for modern development",
  "main": "dist/request-frame-modern.js",
  "jsnext:main": "dist/request-frame-modern.es.js",
  "scripts": {
    "bump": "gulp bump",
    "header": "gulp",
    "build": "./node_modules/.bin/rollup -c rollup/rollup.config.umd.js && rollup -c rollup/rollup.config.es.js",
    "test": "./node_modules/.bin/http-server ./ -p 9999",
    "dummy": "./node_modules/.bin/wzrd  dummy.js | ./node_modules/.bin/wtch | ./node_modules/.bin/garnish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julienetie/request-frame-modern"
  },
  "keywords": [
    "requestAnimationFrame",
    "webkitRequestAnimationFrame",
    "mozRequestAnimationFrame",
    "cancelAnimationFrame",
    "mozCancelAnimationFrame",
    "webkitCancelAnimationFrame",
    "webkitCancelRequestAnimationFrame",
    "setTimeout",
    "cancelTimeout",
    "timing functions",
    "requestAnimationFrame polyfill",
    "polyfill",
    "animation timing",
    "animation timestamp"
  ],
  "author": "Julien Etienne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/julienetie/request-frame-modern/issues"
  },
  "homepage": "https://github.com/julienetie/request-frame-modern",
  "devDependencies": {
    "babel-preset-es2015": "*",
    "babel-preset-es2015-rollup": "*",
    "chai": "*",
    "garnish": "^5.2.0",
    "gulp": "^3.9.1",
    "gulp-bump": "*",
    "gulp-concat": "*",
    "gulp-es-next": "*",
    "gulp-header": "*",
    "gulp-rename": "*",
    "http-server": "*",
    "mocha": "*",
    "requirejs": "*",
    "rollup": "*",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-watch": "*",
    "wtch": "^4.0.1",
    "wzrd": "^1.5.0"
  }
}
